{
    "version": 2,
    "builds": [
        {
            "src": "server.ts",
            "use": "@vercel/node",
            "env": {
                "HTTP_PORT": "3000",
                "GRPC_PORT": "50051"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "server.ts"
        }
    ]
}